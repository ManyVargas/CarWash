@using Application.UsesCases.Usuarios.ObtenerUsuario
@model ObtenerUsuarioResponse
@{
    Layout = null;
   

}

<link href="~/css/loginstyle.css" rel="stylesheet" type="text/css" />
<link href="~/css/globals.css" rel="stylesheet" type="text/css" />


<section id="home">
    <div class="div">
        <div class="overlap">
            <div class="popup">
                <p class="logintext">Login</p>
                <form class="form-login">
                    <label class="idlbl" for="usuarioid">Correo:</label>
                    <input class="idinput" type="text" id="usuarioid" name="usuarioid"><br><br>
                    <label class="passlbl" for="contrasena">Contraseña:</label>
                    <input class="passinput" type="password" id="contrasena" name="contrasena"><br><br>
                    <input class="btnsubmit" type="submit" value="Acceder" asp-controller="Login" asp-action="Admin">
                </form>
            </div>
        </div>
    </div>
</section>


<script src="~/js/script.js"></script>